{
  "name": "split-diff",
  "main": "./lib/split-diff",
  "version": "0.7.0",
  "description": "A split pane diff tool.",
  "keywords": [
    "diff",
    "split",
    "compare",
    "panes",
    "files",
    "lines",
    "code",
    "difference",
    "visual"
